[
{% for p in site.pages.all %}
    {
        "title": "{{ p.title }}",
        "content": {{ p.content|striptags|json_encode }},
        "date": "{{ p.date }}",
        "href": "{{ url(p,{canonical:true}) }}",
        "objectID": "{{ p.id }}"
    }{% if not loop.last %},{% endif ~%}
{% endfor %}
]
